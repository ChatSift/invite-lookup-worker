{
  "compilerOptions": {
    "strict": true,
    "alwaysStrict": true,
    "moduleResolution": "node",
    "removeComments": true,
    "pretty": true,
    "module": "CommonJS",
    "target": "ES2020",
    "lib": ["ESNext", "WebWorker"],
    "types": ["@cloudflare/workers-types"],
    "sourceMap": true,
    "incremental": true,
    "skipLibCheck": true,
    "noEmitHelpers": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "noUncheckedIndexedAccess": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "outDir": "./dist",
    "noEmit": true
  },
  "include": ["./src/**/*.ts"]
}
